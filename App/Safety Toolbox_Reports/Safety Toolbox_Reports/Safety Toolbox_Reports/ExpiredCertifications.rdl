  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CutoffDate">
            <Value>=Parameters!CutoffDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP (1000) 
  CertType, TrainedOnDate, ExpiryDate, EmployeeFirstName, EmployeeLastName
  FROM [SafetyToolBox].[dbo].[Certifications]
  LEFT JOIN Employees ON Certifications.EmployeeID = Employees.EmployeeID

  WHERE ExpiryDate &lt;= @CutoffDate</CommandText>
      </Query>
